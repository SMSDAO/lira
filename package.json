{
  "name": "lira",
  "version": "1.0.0",
  "description": "Lira - Redesigned Zora Protocol with Quantum Oracle & Parallel Agent Execution",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run env:validate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "contracts:compile": "hardhat compile",
    "contracts:test": "hardhat test",
    "contracts:deploy": "hardhat run scripts/deploy.js",
    "php:serve": "php -S localhost:8000 -t backend/php/public",
    "go:build": "cd backend/go && go build -o bin/lira-api ./cmd/api",
    "java:build": "cd backend/java && mvn clean package",
    "env:generate": "ts-node scripts/generate-env.ts",
    "env:generate:beta": "ts-node scripts/generate-env.ts beta",
    "env:generate:prod": "ts-node scripts/generate-env.ts production",
    "env:validate": "ts-node scripts/validate-env.ts",
    "wallet:generate": "ts-node scripts/generate-admin-wallet.ts",
    "faucet:claim": "ts-node scripts/auto-claim.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@prisma/client": "^5.22.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@tanstack/react-query": "^5.17.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "ethers": "^6.9.0",
    "framer-motion": "^10.18.0",
    "next": "^16.1.5",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "recharts": "^2.10.3",
    "tailwindcss": "^3.4.1",
    "viem": "^2.7.0",
    "wagmi": "^2.5.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "chai": "^4.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "hardhat": "^2.19.4",
    "hardhat-gas-reporter": "^1.0.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prisma": "^5.22.0",
    "solidity-coverage": "^0.8.17",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
