    "framer-motion": "^10.18.0",
    "next": "^14.0.4",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "recharts": "^2.10.3",
